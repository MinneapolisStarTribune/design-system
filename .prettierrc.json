{
  "arrowParens": "always",
  "endOfLine": "lf",
  "jsonRecursiveSort": true,
  "jsonSortOrder": "{ \"/^\\\\d+/\": \"numeric\", \"/.*/\": \"lexical\" }",
  "plugins": ["prettier-plugin-sort-json"],
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
