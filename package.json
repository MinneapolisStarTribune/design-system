  "scripts": {
    "tokens": "node scripts/build-tokens.js",
    "icons": "node scripts/build-icon-mapping.js",
    "build": "npm run tokens && npm run icons && NODE_ENV=production vite build",
    "storybook": "npm run tokens && npm run icons && storybook dev -p 6006",
    "build-storybook": "npm run tokens && npm run icons && storybook build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "vitest run a11y.test",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npx husky"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
